<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/21DB</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">toor</property>
    <mapping resource="com/cineplex/entity/Film.hbm.xml"/>
    <mapping resource="com/cineplex/entity/Holiday.hbm.xml"/>
    <mapping resource="com/cineplex/entity/Jadwal.hbm.xml"/>
    <mapping resource="com/cineplex/entity/Pesanan.hbm.xml"/>
    <mapping resource="com/cineplex/entity/Member.hbm.xml"/>
    <mapping resource="com/cineplex/entity/DetailPesanan.hbm.xml"/>
    <mapping resource="com/cineplex/entity/Studio.hbm.xml"/>
    <mapping resource="com/cineplex/entity/Saldo.hbm.xml"/>
    <mapping resource="com/cineplex/entity/Bioskop.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
